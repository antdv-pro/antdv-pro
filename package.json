{
  "name": "antdv-pro",
  "type": "module",
  "version": "0.0.18",
  "packageManager": "pnpm@8.4.0",
  "author": "aibayanyu <aibayanyu@qq.com>",
  "repository": "antdv-pro/antdv-pro",
  "scripts": {
    "dev:admin": "vite",
    "dev": "run-p dev:admin dev:mock",
    "dev:mock": "pnpm -F servers dev",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --fix",
    "bump:patch": "changelogen --bump --output CHANGELOG.md --release --patch",
    "bump:minor": "changelogen --bump --output CHANGELOG.md --release --minor",
    "bump:major": "changelogen --bump --output CHANGELOG.md --release --major",
    "dir-tree": "esno ./scripts/dir-tree",
    "gen:uno": "esno ./scripts/gen-unocss"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@ctrl/tinycolor": "^3.6.0",
    "@v-c/utils": "^0.0.23",
    "@vueuse/core": "^10.2.1",
    "ant-design-vue": "^4.0.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.4",
    "vue": "^3.3.4",
    "vue-i18n": "9.3.0-beta.24",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@mistjs/eslint-config-vue": "^0.0.5",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.195",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^18.16.19",
    "@types/treeify": "^1.0.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "antdv-component-resolver": "^1.0.1",
    "changelogen": "^0.5.4",
    "cross-env": "^7.0.3",
    "directory-tree": "^3.5.1",
    "eslint": "^8.45.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.1",
    "less": "^4.1.3",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "treeify": "^1.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "unocss": "^0.51.13",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.4",
    "vue-tsc": "^1.8.5"
